<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nave_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.loveinshayari.ezywallet.login_singin_Fragments.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_singupFragment"
            app:destination="@id/singupFragment"
            app:exitAnim="@anim/fade_out"
            app:enterAnim="@anim/sind_in"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/sind_out"
            />
    </fragment>
    <fragment
        android:id="@+id/singupFragment"
        android:name="com.loveinshayari.ezywallet.login_singin_Fragments.SingupFragment"
        android:label="fragment_singup"
        tools:layout="@layout/fragment_singup"
        />
</navigation>